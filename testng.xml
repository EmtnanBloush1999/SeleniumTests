<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Suite1" verbose="1">
    <test name="Demo Test">
        <classes>
            <class name="TestNG.GoogleTest">
                <parameter name="search-query" value="Java"/>
                <methods>
                    <exclude name="assertGoogleLogoExists"/>
                </methods>
            </class>
            <class name="TestNG.SoftAssertionsTests"/>
        </classes>
    </test>
</suite>

